{
  "name": "ionic-conference-app",
  "version": "0.4.1",
  "description": "Ionic Conference App",
  "keywords": [
    "ionic2",
    "angular2",
    "typescript",
    "jwt",
    "ngrx",
    "rxjs"
  ],
  "author": {
    "name": "Damien Dell'Amico",
    "email": "damien.dellamico@gmail.com",
    "url": "https://github.com/ddellamico"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddellamico/ionic2-conference-app.git"
  },
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "watch": "ionic-app-scripts watch",
    "serve:before": "watch",
    "emulate:before": "build",
    "deploy:before": "build",
    "build:before": "build",
    "run:before": "build",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "push": "git push --follow-tags origin master",
    "lint": "tslint \"src/**/*.ts\" --exclude=src/**/*.d.ts",
    "docs": "typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "compile:ts": "rm -rf ./build/* && tsc --outDir ./build",
    "outdated": "npm outdated --depth 0"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "web"
      }
    },
    "ios:dev": {
      "command": "ionic run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "npm run build && ionic build ios",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "npm run build && ionic build android",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ionic/storage": "1.1.7",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "^3.2.3",
    "angular2-jwt": "0.1.23",
    "ionic-angular": "2.0.1",
    "ionic-native": "2.4.1",
    "ionicons": "3.0.0",
    "lodash-es": "^4.16.2",
    "ngrx-store-freeze": "0.1.4",
    "ngrx-store-logger": "0.1.7",
    "rxjs": "5.0.0-beta.12",
    "sw-toolbox": "3.4.0",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "1.0.0",
    "typescript": "^2.0.3",
    "@types/google-maps": "^3.1.28",
    "@types/jasmine": "2.5.35",
    "@types/lodash": "^4.14.37",
    "@types/moment": "^2.13.0",
    "better-npm-run": "0.0.11",
    "codelyzer": "^1.0.0-beta.2",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^1.3.2",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-rollup-plugin": "^0.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "phantomjs-prebuilt": "^2.1.13",
    "protractor": "^4.0.9",
    "protractor-jasmine2-screenshot-reporter": "^0.3.2",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-angular": "^0.4.2",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "standard-version": "^3.0.0",
    "ts-helpers": "^1.1.2",
    "ts-node": "^1.4.3",
    "validate-commit-msg": "^2.8.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ionic_rollup": "./config/rollup.config.js"
  },
  "cordovaPlugins": [
    "cordova-plugin-whitelist@1.2.2",
    {
      "locator": "https://github.com/VersoSolutions/CordovaClipboard",
      "id": "com.verso.cordova.clipboard"
    },
    "cordova-plugin-splashscreen@3.2.2",
    "cordova-plugin-crosswalk-webview@1.7.0",
    "cordova-plugin-inappbrowser@1.4.0"
  ],
  "cordovaPlatforms": [
    "ios",
    "android"
  ]
}
